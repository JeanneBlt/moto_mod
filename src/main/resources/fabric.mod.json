		],
		"client": [
			"com.pyloufass.motomod.MotoModClient"
		]
	},
	"mixins": [
		"motomod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
