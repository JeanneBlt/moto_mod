{
	"schemaVersion": 1,
	"id": "motomod",
	"version": "${version}",
	"name": "Moto Mod",
	"description": "A mod only about moto...",
	"authors": [
		"Tris, Pyloufass"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/motomod/icon.png",
